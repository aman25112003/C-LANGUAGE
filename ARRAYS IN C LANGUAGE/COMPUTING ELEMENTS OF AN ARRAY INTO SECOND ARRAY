#include <stdio.h>
#include <conio.h>
int main()
{
    int i,n;
    printf("ENTER THE NUMBER OF ELEMENTS IN THE ARRAY\n");
    scanf("%d\n",&n);
    int A[n],B[n];
    for(i=0;i<n;i++)
    {
        printf("ENTER THE %d ELEMENT OF THE ARRAY:\n",i);
        scanf("%d",&A[i]);
    }
    for(i=0;i<n;i++)
    {
        B[i]=A[i];
    }
    printf("THE ORIGINAL ARRAY IS:\n");
    for(i=0;i<n;i++)
    {
        printf("%d\n",A[i]);
    }
    printf("THE COPIED ARRAY IS:\n");
    for(i=0;i<n;i++)
    {
        printf("%d\n",B[i]);
    }
    return 0;
}
