#include <stdio.h>
#include <conio.h>
int main()
{
    int i,j,n,A[100][100];
    printf("ENTER THE SIZE OF THE MATRIX:\n");
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        for(j=0;j<n;j++)
        {
            printf("ENTER THE [%d][%d] ELEMENT OF THE ARRAY:\n");
            scanf("%d",&A[i][j]);
        }
    }
    printf("THE ENTERED MATRIX IS:\n");
    for(i=0;i<n;i++)
    {
        for(j=0;j<n;j++)
        {
            printf("%d",A[i][j]);
        }
    }
    
}
