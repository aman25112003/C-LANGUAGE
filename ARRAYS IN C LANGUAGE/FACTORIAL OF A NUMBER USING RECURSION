#include <stdio.h>
#include <conio.h>
int factorial(int num)
{
    if(num==0||num==1)
    {
        return 1;
    }
    else
    {
        return (num*factorial(num-1));
    }
}
int main()
{
    int n;
    printf("ENTER THE NUMBER WHOSE FACTORIAL IS NEEDED");
    scanf("%d",&n);
    printf("THE FACTORIAL OF %d IS %d",n,factorial(n));
    return 0;
}
