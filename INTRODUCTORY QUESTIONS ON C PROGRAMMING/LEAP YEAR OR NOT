#include <stdio.h>
#include <conio.h>
int main()
{
    int a;
    printf("ENTER THE YEAR ");
    scanf("%d",&a);
    if(a%4==0)
    {
        printf("ENTERED YEAR IS A LEAP YEAR");
    }
    else if(a%400==0)
    {
        printf("ENTERED YEAR IS A LEAP YEAR");
    }
    else if(a%100==0)
    {
        printf("ENTERED YEAR IS NOT A LEAP YEAR");
    }
    else
    {
        printf("ENTERED YEAR IS NOT A LEAP YEAR");
    }
    return 0;
}
